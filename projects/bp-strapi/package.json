{
  "name": "bp-strapi",
  "private": true,
  "version": "0.1.0",
  "description": "The backend system to manage picture and tag data",
  "scripts": {
    "develop": "cross-env ENV_PATH=./config/env/development/.env strapi develop",
    "start": "cross-env ENV_PATH=./config/env/production/.env strapi start",
    "build": "cross-env ENV_PATH=./config/env/development/.env strapi build",
    "build:prod": "cross-env ENV_PATH=./config/env/production/.env strapi build",
    "strapi": "strapi",
    "test": "cross-env ENV_PATH=./config/env/test/.env jest --forceExit --detectOpenHandles --watchAll",
    "test:ci": "cross-env ENV_PATH=./config/env/test/.env jest --forceExit --detectOpenHandles --verbose"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "@strapi/plugin-graphql": "4.1.9",
    "@strapi/plugin-i18n": "4.1.9",
    "@strapi/plugin-sentry": "^4.1.9",
    "@strapi/plugin-users-permissions": "4.1.9",
    "@strapi/strapi": "4.1.9",
    "cross-env": "^7.0.3",
    "pg": "8.6.0",
    "xlsx": "^0.18.5"
  },
  "author": {
    "name": "BP2021RRH1"
  },
  "strapi": {
    "uuid": "b2b5bab5-6b6e-4feb-ae28-403e43047dd9"
  },
  "engines": {
    "node": ">=14.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache",
      "/build/"
    ],
    "testEnvironment": "node",
    "globalSetup": "./unit-tests/setupTests.js",
    "globalTeardown": "./unit-tests/tearDownTests.js"
  },
  "license": "MIT"
}
