{
  "kind": "collectionType",
  "collectionName": "archive_tags",
  "info": {
    "singularName": "archive-tag",
    "pluralName": "archive-tags",
    "displayName": "Archive_Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "pictures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::picture.picture",
      "mappedBy": "archive_tag"
    },
    "longDescription": {
      "type": "richtext"
    },
    "showcasePicture": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::picture.picture"
    },
    "links": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::link.link",
      "mappedBy": "archive_tag"
    },
    "shortDescription": {
      "type": "text"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "paypalClient": {
      "type": "string"
    },
    "paypalDonationText": {
      "type": "string"
    },
    "paypalPurpose": {
      "type": "string"
    },
    "restrictImageDownloading": {
      "type": "boolean"
    }
  }
}
